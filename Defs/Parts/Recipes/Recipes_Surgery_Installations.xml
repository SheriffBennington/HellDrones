<Defs>
  <RecipeDef ParentName="SurgeryAndroid">
    <defName>InstallOLuciferiumPump</defName>
    <label>install overcharged mechanite pump</label>
    <description>Installs an overcharged mechanite pump.</description>
    <workerClass>MOARANDROIDS.Recipe_InstallArtificialBodyPartAndroid</workerClass>
    <jobString>Installing overcharged mechanite pump.</jobString>
    <skillRequirements>
      <Crafting>12</Crafting>
    </skillRequirements>
    <workAmount>1600</workAmount>
    <surgerySuccessChanceFactor>0.8</surgerySuccessChanceFactor>
    <recipeUsers>
      <li>Android4Tier</li>
      <li>Android5Tier</li>
      <li>AT_HellUnit</li>
    </recipeUsers>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>RepairKitAT</li>
          </thingDefs>
        </filter>
        <count>3</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>OLuciferiumPump</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>RepairKitAT</li>
        <li>OLuciferiumPump</li>
      </thingDefs>
    </fixedIngredientFilter>
    <appliedOnFixedBodyParts>
      <li>MRightKidney</li>
    </appliedOnFixedBodyParts>
    <addsHediff>OLuciferiumPump</addsHediff>
  </RecipeDef>
